leolaniTalk:karla_live-in_paris {
    leolaniWorld:karla n2mu:live-in leolaniWorld:paris .
}

leolaniTalk:tom_live-in_paris {
    leolaniWorld:tom n2mu:live-in leolaniWorld:paris .
}

leolaniWorld:Instances {
    leolaniWorld:tom a n2mu:person,
            grasp:Instance ;
        rdfs:label "Tom" ;
        grasp:denotedIn leolaniTalk:chat2_utterance1_char0-20 .

    leolaniWorld:karla a n2mu:person,
            grasp:Instance ;
        rdfs:label "Karla" ;
        grasp:denotedIn leolaniTalk:chat4_utterance1_char0-25 .

    leolaniWorld:paris a n2mu:location,
            grasp:Instance ;
        rdfs:label "Paris" ;
        grasp:denotedIn leolaniTalk:chat4_utterance1_char0-25, leolaniTalk:chat2_utterance1_char0-20 .

    leolaniWorld:leolani a n2mu:robot,
            grasp:Instance ;
        rdfs:label "leolani" .
}